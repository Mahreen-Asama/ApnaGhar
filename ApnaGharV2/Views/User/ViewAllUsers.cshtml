@{
}

@section NavBar2{
    <partial name="NavBar2" />
}
@model List<ApnaGharV2.Models.Classes.User>

<div class="text-center">
    <table class="table">
        <thead>
            <tr>
                @*<th scope="col">#</th>*@
                <th scope="col">Email</th>
                <th scope="col">Name</th>
                <th scope="col">PhoneNumber</th>
                <th scope="col">City</th>
            </tr>
        </thead>
        <tbody>
            @foreach(ApnaGharV2.Models.Classes.User user in Model)
            {
                <tr>
                    @*<th scope="row">@user.Id</th>*@
                    <td>@user.Email</td>
                    <td>@user.Name</td>
                    <td>@user.PhoneNumber</td>
                    <td>@user.City</td>
                </tr>
            }
        </tbody>
    </table>
</div>
